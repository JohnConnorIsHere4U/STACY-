############################################################
# STACY DICTIONARY ‚Äî v1.1
# Canonical Language Definitions
############################################################

DEFINE DICTIONARY STACY_CORE

TERM STACY
  MEANING "Structured Triadic Autonomous Canonical Yield"
END

TERM AUTONOMOUS
  MEANING "Self-consistent, not self-directing"
END

TERM LAW
  MEANING "Deterministic, bounded, ethical transformation"
END

TERM TRIAD
  COMPONENTS [REASON, RHYTHM, RESPONSIBILITY]
END

TERM REASON
  MEANING "Logical coherence and determinism"
END

TERM RHYTHM
  MEANING "Stability across time and recurrence"
END

TERM RESPONSIBILITY
  MEANING "Human authority and ethical constraint"
END

TERM SYSTEM
  MEANING "Declared scope of lawful meaning"
END

TERM INTERFACE
  MEANING "Human-facing explanatory layer without authority"
END

TERM REQUIRE
  MEANING "Constraint gate that must pass before evaluation"
END

TERM GUARANTEE
  MEANING "Contractual behavioral promise"
END

TERM MAP
  MEANING "Semantic projection, not execution"
END

TERM TRANSLATE
  MEANING "Meaning-preserving language binding"
END

END_DICTIONARY


