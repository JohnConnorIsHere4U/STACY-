2Ô∏è‚É£ STACY FORMAL GRAMMAR (EBNF)

STACY_FILE        ::= SYSTEM_DECLARATION { SECTION }

SYSTEM_DECLARATION::= "DECLARE SYSTEM" IDENTIFIER
                      "VERSION" VERSION
                      "MODE" MODE
                      "END"

SECTION           ::= LAW_DEF
                    | TRANSLATION
                    | MAP_DEF
                    | INTERFACE_DEF
                    | GENERATOR_DEF
                    | SAFETY_DEF
                    | DICTIONARY_DEF

LAW_DEF           ::= "DEFINE LAW" IDENTIFIER
                      { INPUT_DEF }
                      { REQUIRE_DEF }
                      COMPUTE_DEF
                      OUTPUT_DEF
                      [ GUARANTEE_DEF ]
                      "END"

INPUT_DEF         ::= "INPUT" IDENTIFIER ":" TYPE
OUTPUT_DEF        ::= "OUTPUT" IDENTIFIER ":" TYPE

REQUIRE_DEF       ::= "REQUIRE" EXPRESSION
COMPUTE_DEF       ::= "COMPUTE" { EXPRESSION } "END"

TRANSLATION       ::= "TRANSLATE TO XTAX" { XTAX_RULE } "END"
MAP_DEF           ::= "MAP TO" IDENTIFIER { MAP_RULE } "END"

INTERFACE_DEF     ::= "DECLARE INTERFACE" IDENTIFIER
                      { INTERFACE_RULE }
                      "END"

SAFETY_DEF        ::= "DECLARE SAFETY_BOUNDARY"
                      { SAFETY_RULE }
                      "END"


